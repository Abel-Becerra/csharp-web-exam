{
  "Environment": "QA",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=app_data/app_qa.db"
  },
  "ApiSettings": {
    "EnableSwagger": true,
    "EnableDetailedErrors": true,
    "EnableCors": true,
    "CorsOrigins": "https://qa.example.com,https://qa-ui.example.com",
    "MaxPageSize": 50,
    "DefaultPageSize": 10
  },
  "AllowedHosts": "qa.example.com;qa-api.example.com"
}
