{
  "Environment": "Production",
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "System": "Error",
      "Microsoft": "Error",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=app_data/app_prod.db"
  },
  "ApiSettings": {
    "EnableSwagger": false,
    "EnableDetailedErrors": false,
    "EnableCors": true,
    "CorsOrigins": "https://example.com,https://www.example.com",
    "MaxPageSize": 50,
    "DefaultPageSize": 10
  },
  "AllowedHosts": "example.com;api.example.com;www.example.com"
}
